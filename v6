}

function isTeamMessage(player, message)
{
	if (message.startsWith("!t"))
	{
		var teamList = room.getPlayerList().filter((gracz)=> gracz.team == player.team && gracz.team != 0);
		
		var teamName = (player.team == 1) ? "RED" : "BLUE";
		
		for(var i=0; i<teamList.length; i++)
			room.sendAnnouncement("["+teamName+ " - " + player.name + "] " + message.substr(2) , teamList[i].id , player.team == 1 ? "0xFF4640" : "0x089CFF", "bold");
		return false;
	}
	
	return true;
}

