    }
}

var mutedPlayers = []; // Oyuncu sessiz/sesli komutlar.

var commands = {
	// Admin olma komutu.
	"!duce": adminFun,
    "!swap": swapFun, 
    "!cb": clearbansFun,
    "!rr": resetFun,


	// KullanÄ±cÄ±ya sohbeti kapatmak iÃ§in !mute OyuncuVeyaAdmin. SusuturmayÄ± kaldÄ±rmak iÃ§in -unmute OyuncuVeyaAdmin Komutunu girin.
	"-mute" : pushMuteFun,
	"-unmute": unmuteFun
}

	
function pushMuteFun(player, message){
    var susturulcak=message.substr(6);
        if (player.admin == true){
        if (player.name==susturulcak) {
        //room.sendChat("Kendini Muteleyemezsin.");
        return;
        }
        if (GetPlayerByName(susturulcak)[0] !=null) {
        if (GetPlayerByName(susturulcak)[0].admin == true) {
        //room.sendChat("Admini susturamazsÄ±n!");
        return;
        }
        if (!mutedPlayers.includes(susturulcak)) { }else { return;}
        }else if (!(mutedPlayers.includes(susturulcak))) {}else {}
            if (!(mutedPlayers.includes(susturulcak))) { mutedPlayers.push(susturulcak);}
        }
    }

 
function swapFun(player){
    if (player.admin == true){
        if (room.getScores() == null) {
            players = room.getPlayerList();
            for (i = 0; i < players.length; i++){
                if (players[i].team == 1){
                    room.setPlayerTeam(players[i].id, 2);
                }
                else if (players[i].team == 2){
                    room.setPlayerTeam(players[i].id, 1);
                }
            }
        }
    }
}



function updateAdmins() {
  // Get all players except the host (id = 0 is always the host)
  var players = room.getPlayerList().filter((player) => player.id != 0 );
  if ( players.length == 0 ){room.stopGame();} // No players left, do nothing.
  if ( players.find((player) => player.admin) != null ) return; // There's an admin left so do nothing.
  room.setPlayerAdmin(players[0].id, true); // Give admin to the first non admin player in the list
}


function clearbansFun(player){ // !clear
    if (player.admin == true){ room.clearBans(); room.sendChat("ð“ð”ðŒ ðð€ðð‹ð€ð‘ ðŠð€ð‹ðƒðˆð‘ðˆð‹ðƒðˆ!");}
}



function Limpiarbans() {
    var scores = room.getScores();
    if (scores.time > 3 && !isTimeAddedShownquince) {
    room.clearBans();
    isTimeAddedShownquince = true;
    }
}


function resetFun(player){
    if (player.admin == true){
        room.stopGame();
        room.startGame();
    }
}



function gotMutedFun(player){
	if (mutedPlayers.includes(player.name)){
		return true;
	}
}

function unmuteFun(player, message, mesajj){ // !unmute OyuncuVeyaAdmin
	// Birinin sesi kapatÄ±lmÄ±ÅŸsa konuÅŸmasÄ±na izin ver.
	// YÃ¶netici olmanÄ±z gerekir.
	if (player.admin == true){
		pos = mutedPlayers.indexOf(message.substr(9));
		mutedPlayers.splice(pos, 1);
    		 room.sendChat(player.name + " tarafÄ±ndan " + message.substr(8) + ' AdlÄ± KullanÄ±cÄ±ya Sohbet AÃ§Ä±ldÄ±.');
return false;
	}
}


